for x in range(2, 11, 2):
    print(x)

for x in range(3, 16, 3):
    print(x)

for x in range(4, 21, 4):
    print(x)

for x in range(5,26,5):
    print(x)

#nested loop sample
for y in range(1,11):
    for z in range(1,11):
        print(z * y,end='\t')
    print()

# nested loop sample
for y in range(10,21):
    for z in range(1,11):
        print(z * y,end='\t')
    print()
